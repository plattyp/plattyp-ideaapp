<%= simple_form_for [@idea, @feature, Subfeature.new] do |s| %>
	<%= s.input :name %>
	<%= s.input :description %>
	<%= s.input :subfeaturecategory_id, collection: @uniquecategories,:label => "Category", include_blank: false %>
	<%= s.input :status, collection: @subfeaturestatuses,:label => "Status", include_blank: false  %>
	<%= s.button :submit %>
<% end %>